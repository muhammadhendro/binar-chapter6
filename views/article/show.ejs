<html>
  <head>
    <title>Articles | All</title>
  </head>
  <body>
    <% article.forEach(i => { %>
    <div>
      <h1><%= i.title %></h1>
      <p><%= i.id %></p>
      <p><%= i.body %></p>
      <a href="/views/article/show/<%= i.id %>">Baca lebih lanjut!</a>
    </div>
    <% }) %>
  </body>
</html>
